  </servlet>
  <servlet-mapping>
  	<servlet-name>CreateNoteServlet</servlet-name>
  	<url-pattern>/CreateNoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServlet</servlet-name>
  	<url-pattern>/DeleteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
